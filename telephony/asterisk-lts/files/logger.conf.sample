; $OpenBSD: logger.conf.sample,v 1.3 2007/09/05 22:42:52 sthen Exp $
; log files created in directory defined by 'astlogdir' in asterisk.conf

[logfiles]
console => notice,warning,error
messages => notice,warning,error
;syslog.local0 => notice,warning,error

;debug => debug
;full => notice,warning,error,debug,verbose

[general]
;appendhostname = yes
;dateformat=%F %T
;  see strftime(3)

;queue_log = no
;event_log = no
